{
  "name": "pedrosoares/lumen-request-logger",
  "description": "HTTP request logger middleware for Laravel",
  "keywords": ["laravel", "lumen", "logger", "request", "response"],
  "license": "MIT",
  "authors": [
    {
      "name": "Pedro Augusto da Silva Soares",
      "homepage": "http://pedrosoares.net",
      "email": "contato@pedrosoares.net",
      "role": "Developer"
    },{
      "name": "Anderson Andrade",
      "homepage": "http://andersonandra.de",
      "email": "contato@andersonandra.de",
      "role": "Developer"
    }
  ],
  "homepage": "http://andersao.github.io/laravel-request-logger",
  "support": {
    "email": "contato@andersonandra.de",
    "issues":"https://github.com/andersao/laravel-request-logger/issues",
    "wiki":"https://github.com/andersao/laravel-request-logger",
    "source":"https://github.com/andersao/laravel-request-logger",
    "docs": "http://andersao.github.io/laravel-request-logger"
  },
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "~5.0",
    "illuminate/http": "~5.0",
    "monolog/monolog": "~1.11"
  },
  "require-dev" : {
    "laravel/framework": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Prettus\\RequestLogger\\": "src/Pedrosoares/RequestLogger/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Prettus\\RequestLogger\\Providers\\LoggerServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-develop": "1.2-dev"
    }
  },
  "minimum-stability": "stable"
}
